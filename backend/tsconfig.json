{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "ts-node": {
    "transpileOnly": true,
    "skipIgnore": true,
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true
    }
  },
  "include": [
    "lib/**/*",
    "bin/**/*"
, "legacy/lib/ada-clara-scraper-stack.ts"  ],
  "exclude": [
    "node_modules",
    "dist",
    "lambda",
    "legacy/lambdaslambdas",
    "scripts",
    "test",
    "legacy"
  ]
}