{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy-s3-vectors-ga": "cdk deploy AdaClaraS3VectorsGA --app \"npx ts-node scripts/deploy-s3-vectors-ga.ts\"",
    "create-dynamodb-tables": "npx ts-node scripts/create-dynamodb-tables.ts",
    "test-dynamodb": "npx ts-node scripts/test-dynamodb-setup.ts",
    "test-dynamodb-deployment": "npx ts-node scripts/test-dynamodb-deployment.ts",
    "test-s3": "npx ts-node scripts/test-s3-integration.ts",
    "test-data-service": "npx ts-node scripts/test-data-service.ts",
    "deploy-chat-processor": "cdk deploy --app \"npx ts-node scripts/deploy-chat-processor.ts\"",
    "test-chat-processor": "npx ts-node scripts/test-chat-processor.ts",
    "test-workflow": "ts-node scripts/test-full-workflow.ts",
    "simulate-workflow": "ts-node scripts/simulate-workflow.ts",
    "cleanup-test": "ts-node scripts/cleanup-test-resources.ts",
    "deploy-ses-escalation": "cdk deploy AdaClaraSESEscalation --app \"npx ts-node scripts/deploy-ses-escalation.ts\"",
    "test-escalation": "npx ts-node scripts/test-escalation-workflow.ts",
    "deploy-admin-analytics": "cdk deploy AdaClaraAdminAnalytics --app \"npx ts-node scripts/deploy-admin-analytics.ts\"",
    "test-admin-analytics": "npx ts-node scripts/test-admin-analytics.ts",
    "deploy-enhanced-dynamodb": "cdk deploy AdaClaraEnhancedDynamoDB --app \"npx ts-node scripts/deploy-enhanced-dynamodb.ts\"",
    "test-enhanced-dynamodb": "npx ts-node scripts/test-enhanced-dynamodb.ts",
    "create-enhanced-tables": "npx ts-node scripts/create-enhanced-tables.ts",
    "test-analytics-simple": "npx ts-node scripts/test-analytics-simple.ts",
    "deploy-enhanced-admin-api": "npx ts-node scripts/deploy-enhanced-admin-api.ts",
    "test-enhanced-api": "npx ts-node scripts/test-enhanced-api-endpoints.ts",
    "test-escalation-analytics": "npx ts-node scripts/test-escalation-analytics.ts",
    "test-faq-analysis": "npx ts-node scripts/test-faq-analysis.ts",
    "deploy-enhanced-faq": "npx ts-node scripts/deploy-enhanced-faq.ts",
    "test:comprehensive": "jest test/comprehensive --testTimeout=30000",
    "test:unit": "jest test/comprehensive/unit --testTimeout=10000",
    "test:integration": "jest test/comprehensive/integration --testTimeout=20000",
    "test:performance": "npx ts-node scripts/test-task6-2-performance-validation.ts",
    "test:e2e": "jest test/comprehensive/e2e --testTimeout=60000",
    "cleanup-experimental": "npx ts-node scripts/cleanup-experimental-stacks.ts",
    "cleanup-experimental-live": "npx ts-node scripts/cleanup-experimental-stacks.ts --execute",
    "cleanup-tests": "npx ts-node scripts/cleanup-experimental-tests.ts",
    "cleanup-tests-live": "npx ts-node scripts/cleanup-experimental-tests.ts --execute",
    "cleanup-docs": "npx ts-node scripts/cleanup-experimental-docs.ts",
    "cleanup-docs-live": "npx ts-node scripts/cleanup-experimental-docs.ts --execute",
    "cleanup-all": "npm run cleanup-experimental-live && npm run cleanup-tests-live && npm run cleanup-docs-live",
    "deploy-production": "npx ts-node scripts/deploy-production-complete.ts",
    "deploy-production-core": "npx ts-node scripts/deploy-production-complete.ts --core-only",
    "deploy-unified-api": "cdk deploy AdaClaraUnifiedAPI --app \"npx ts-node scripts/deploy-unified-api.ts\"",
    "deploy-core-unified-api": "cdk deploy --all --app \"npx ts-node scripts/deploy-core-unified-api.ts\"",
    "deploy-minimal-api": "cdk deploy --all --app \"npx ts-node scripts/deploy-minimal-api.ts\"",
    "test-unified-api": "npx ts-node scripts/test-unified-api.ts",
    "get-api-config": "npx ts-node scripts/get-api-config.ts",
    "test:production": "npx ts-node scripts/run-comprehensive-tests.ts",
    "test:infrastructure": "npx ts-node scripts/test-ga-infrastructure-simple.ts",
    "test:deployment": "npx ts-node scripts/test-rag-processor-simple.ts && npx ts-node scripts/test-analytics-simple.ts",
    "deploy-core": "npx ts-node scripts/deploy-fresh-complete.ts --core-only",
    "remove-bloat": "npx ts-node scripts/remove-codebase-bloat.ts",
    "remove-bloat-execute": "npx ts-node scripts/remove-codebase-bloat.ts --execute"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1100.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.958.0",
