    "@aws-sdk/client-cloudformation": "^3.958.0",
    "@aws-sdk/client-cloudwatch": "^3.958.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.958.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.958.0",
    "@aws-sdk/client-comprehend": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/client-eventbridge": "^3.958.0",
    "@aws-sdk/client-iam": "^3.958.0",
    "@aws-sdk/client-lambda": "^3.700.0",
    "@aws-sdk/client-opensearchserverless": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-s3vectors": "^3.958.0",
    "@aws-sdk/client-ses": "^3.958.0",
    "@aws-sdk/client-sns": "^3.958.0",
    "@aws-sdk/client-sqs": "^3.958.0",
    "@aws-sdk/lib-dynamodb": "^3.958.0",
    "@exodus/bytes": "^1.7.0",
    "aws-cdk-lib": "2.233.0",
    "axios": "^1.13.2",
    "cdk-s3-vectors": "^0.3.2",
    "cheerio": "^1.0.0",
    "constructs": "^10.0.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "playwright-core": "^1.48.0"
  },
  "overrides": {
    "whatwg-encoding": "npm:@exodus/bytes@^1.0.0"
  }
}
