{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "test-crawlers-local": "ts-node scripts/local-crawler-test.ts",
    "deploy-crawler-test": "cdk deploy WebCrawlerTestStack",
    "test-crawlers-aws": "ts-node scripts/test-crawlers.ts",
    "deploy-bedrock-crawler": "cdk deploy BedrockWebCrawlerStack --app 'ts-node scripts/deploy-bedrock-crawler.ts'",
    "test-bedrock-full": "ts-node scripts/test-bedrock-crawler.ts full",
    "test-bedrock-queries": "ts-node scripts/test-bedrock-crawler.ts queries",
    "test-bedrock-compare": "ts-node scripts/test-bedrock-crawler.ts compare",
    "deploy-s3-vectors": "cdk deploy S3VectorsCrawlerStack --app 'ts-node scripts/deploy-s3-vectors.ts'",
    "deploy-s3-vectors-minimal": "cdk deploy AdaClaraS3VectorsMinimal --app 'ts-node scripts/deploy-s3-vectors-minimal.ts'",
    "deploy-s3-vectors-official": "cdk deploy AdaClaraS3Vectors --app 'npx ts-node scripts/deploy-s3-vectors-official.ts'",
    "deploy-s3-vectors-infra": "cdk deploy AdaClaraS3VectorsInfra --app 'npx ts-node scripts/deploy-s3-vectors-infra-only.ts'",
    "deploy-s3-vectors-bucket-only": "cdk deploy AdaClaraS3VectorsBucketOnly --app 'npx ts-node scripts/deploy-s3-vectors-bucket-only.ts'",
    "test-s3-vectors-full": "ts-node scripts/test-s3-vectors.ts full",
    "test-s3-vectors-crawler": "ts-node scripts/test-s3-vectors.ts crawler",
    "test-s3-vectors-kb": "ts-node scripts/test-s3-vectors.ts kb",
    "test-workflow": "ts-node scripts/test-full-workflow.ts",
    "simulate-workflow": "ts-node scripts/simulate-workflow.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1100.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.700.0",
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@aws-sdk/client-lambda": "^3.700.0",
    "@aws-sdk/client-opensearchserverless": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "aws-cdk-lib": "2.233.0",
    "axios": "^1.7.0",
    "cdk-s3-vectors": "^0.1.11",
    "cheerio": "^1.0.0",
    "constructs": "^10.0.0",
    "playwright-core": "^1.48.0"
  }
}
